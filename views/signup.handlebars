<h2>Sign Up</h2>

<form class="signup-form">
    <div>
        <label for="username-signup">Username:</label>
        <input type="text" id="username-signup" />
    </div>
    <div>
        <label for="email-signup">Email:</label>
        <input type="text" id="email-signup" />
    </div>
    <div>
        <label for="password-signup">Password:</label>
        <input type="password" id="password-signup" />
    </div>
        <div>
        <form class="age" action="../" onsubmit="if (this.MyCheckbox.checked == false) { alert ('You didn\'t choose any of the checkboxes!'); return false; } else { return true; }">
            <p id="age-quest">Are you 21+?<p>
            <p><input  id="age-signup" type="CHECKBOX" name="MyCheckbox" value="true"> Yes</p>
            <p><input  id="age-signup" type="CHECKBOX" name="MyCheckbox" value="false"> No</p>
        </form>
    </div>
    <div>
        <button type="submit">signup</button>
    </div>
</form>

<script src="/javascript/signup.js"></script>